{
  "name": "aor-jsonapi-client",
  "version": "0.1.16-rc.0",
  "description": "JSON API REST Client for Admin-on-rest",
  "main": "./src/restClient.js",
  "scripts": {
    "build": "npm run-script babel1 && npm run-script babel2 && npm run-script babel3 && npm run-script babel4",
    "babel1": "babel --presets es2015 src/restClient.js -o build/restClient.js",
    "babel2": "babel --presets es2015 src/fetch.js -o build/fetch.js",
    "babel3": "babel --presets es2015 src/types.js -o build/types.js",
    "babel4": "babel --presets es2015 src/HttpError.js -o build/HttpError.js",
    "test": "echo \\\\\\\"Error: no test specified\\\\\\\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moonlight-labs/aor-jsonapi-client.git"
  },
  "keywords": [
    "react",
    "admin-on-rest",
    "json-api",
    "rest-client"
  ],
  "author": "Max Schridde",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/moonlight-labs/aor-jsonapi-client/issues"
  },
  "homepage": "https://github.com/moonlight-labs/aor-jsonapi-client#readme",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0"
  }
}
